filter() {
	case "$1" in
		"GitLab")
			awk -F '\t' 'BEGIN { OFS = "\t"; }
				$2 ~ /^Gaspar Vardanyan pushed to project branch master at Gaspar Vardanyan/ {
					next;
				}
				{
					print $0
				}'
		;;
		*)
			cat
		;;
	esac
}
